{
  "name": "integrity",
  "version": "1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
      "devx": "npm run prep && npm run dev",
    "prep": "npm run squark && npm run export",
      "prepx": "npm run squarkx && npm run export",
    "squark": "cd ../.stranger-quarkdown && rake squark[fonts,scss] && cd ../site",
      "squarkx": "cd ../.stranger-quarkdown && rake squark[fonts,scss,assets] && cd ../site",
    "export": "cd ../scripts && rake export && cd ../site",
    "build": "vite build",
      "buildx": "npm run prepx && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "katex": "^0.16.11",
    "sass": "^1.77.6",
    "svelte": "^5.0.0",
    "svelte-preprocess": "^6.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "type": "module"
}
