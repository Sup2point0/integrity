{
  "name": "integrity",
  "version": "1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
      "devx": "npm run prep && vite --open",
    "prep": "npm run squark && npm run export",
      "prepx": "npm run squarkx && npm run export",
    "squark": "cd ../.stranger-quarkdown && rake squark[fonts,scss] && cd ../site",
      "squarkx": "cd ../.stranger-quarkdown && rake squark[fonts,scss,assets] && cd ../site",
    "export": "cd ../.github/scripts && rake export && cd ../../site",
    "build": "vite build",
      "buildx": "npm run prepx && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@stdlib/random-sample": "^0.2.1",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.12",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "fuzzball": "^2.1.3",
    "javascript-time-ago": "^2.5.10",
    "katex": "^0.16.11",
    "mdsvex": "^0.12.3",
    "rehype-katex": "^7.0.1",
    "rehype-katex-svelte": "^1.2.0",
		"rehype-slug": "^6.0.0",
    "remark-footnotes": "^2.0.0",
    "remark-github-blockquote-alert": "1.0",
    "remark-math": "^3.0.0",
    "remark-numbered-footnote-labels": "^1.1.0",
    "sass": "^1.77.6",
    "svelte": "^5.0.0",
    "svelte-persisted-store": "^0.8.1",
    "svelte-preprocess": "^6.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^6.0.0"
  },
  "type": "module"
}
